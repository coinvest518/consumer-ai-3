{
  "version": 2,
  "builds": [
    { "src": "api/upload.ts", "use": "@vercel/node" },
    { "src": "api/stripe-webhook.ts", "use": "@vercel/node" },
    { "src": "api/send-questionnaire.ts", "use": "@vercel/node" },
    { "src": "api/email-cta.ts", "use": "@vercel/node" },
    { "src": "api/dashboard.ts", "use": "@vercel/node" },
    { "src": "api/daily-login-bonus.ts", "use": "@vercel/node" },
    { "src": "api/credit-builder-track.ts", "use": "@vercel/node" },
    { "src": "api/create-checkout-session.ts", "use": "@vercel/node" },
    { "src": "api/templates.ts", "use": "@vercel/node" },
    { "src": "api/user.ts", "use": "@vercel/node" },
    { "src": "api/health.js", "use": "@vercel/node" },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/api/credit-builder/?", "dest": "/api/credit-builder.ts" },
    { "src": "/api/credit-builder-track/?", "dest": "/api/credit-builder-track.ts" },
    { "src": "/api/daily-login-bonus/?", "dest": "/api/daily-login-bonus.ts" },
    { "src": "/api/create-checkout-session/?", "dest": "/api/create-checkout-session.ts" },
    { "src": "/api/stripe-webhook/?", "dest": "/api/stripe-webhook.ts" },
    { "src": "/api/upload/?", "dest": "/api/upload.ts" },
    { "src": "/api/user/credits/?", "dest": "/api/user.ts" },
    { "src": "/api/user/files/?", "dest": "/api/user.ts" },
    { "src": "/api/templates/save/?", "dest": "/api/templates.ts" },
    { "src": "/api/templates/list/?", "dest": "/api/templates.ts" },
    { "src": "/api/templates/delete/?", "dest": "/api/templates.ts" },
    { "src": "/api/email-cta/?", "dest": "/api/email-cta.ts" },
    { "src": "/api/health/?", "dest": "/api/health.js" },
    { "src": "/api/(.*)", "dest": "https://consumer-ai-render.onrender.com/$1" },

    { "src": "/sitemap.xml", "dest": "/sitemap.xml" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/(.*\\.(js|css|ico|png|jpg|svg))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}